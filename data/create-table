CREATE TABLE IF NOT EXISTS public.relics
(
    relics_tier character varying(10),
    relics_relicname character varying(10),
    relics_state character varying(40),
    relics_rewards_0_id character varying(40),
    relics_rewards_0_itemname character varying(40),
    relics_rewards_0_rarity character varying(20),
    relics_rewards_0_chance numeric(4,2),
    relics_rewards_1_id character varying(40),
    relics_rewards_1_itemname character varying(40),
    relics_rewards_1_rarity character varying(20),
    relics_rewards_1_chance numeric(4,2),
    relics_rewards_2_id character varying(40),
    relics_rewards_2_itemname character varying(40),
    relics_rewards_2_rarity character varying(20),
    relics_rewards_2_chance numeric(4,2),
    relics_rewards_3_id character varying(40),
    relics_rewards_3_itemname character varying(40),
    relics_rewards_3_rarity character varying(20),
    relics_rewards_3_chance numeric(4,2),
    relics_rewards_4_id character varying(40),
    relics_rewards_4_itemname character varying(40),
    relics_rewards_4_rarity character varying(20),
    relics_rewards_4_chance numeric(4,2),
    relics_rewards_5_id character varying(40),
    relics_rewards_5_itemname character varying(40),
    relics_rewards_5_rarity character varying(20),
    relics_rewards_5_chance numeric(4,2),
    relics_id character varying(40)
);
